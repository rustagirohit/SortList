       ,[NoOfRecords]=@NoOfRecords
      ,[RegionWiseKRIPerformance]=@RegionWiseKRIPerformance
      ,[RagFlag]=@RagFlag
      ,[KRIRemark]=@KRIRemark
      ,[PerformanceInfo]=@PerformanceInfo,
	  modifiedBy=@CreatedBy,ModifiedDate=GETUTCDATE()
	  where RasMetricId=@RasMetricId
