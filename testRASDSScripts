		---ZoneContext
		if(@ZoneContextId >0)
		 BEGIN

		 ---Region
		 if(@ZoneContextId = 1 and @RegionId>0)
		 BEGIN

		 if(@NumeratorWhereSQL <> '' and len(@NumeratorWhereSQL)>0)
		 begin
		 SET @NumeratorWhereSQL= @NumeratorWhereSQL + ' and Region=''' + @RegionName + '''';
		 end
		 else
