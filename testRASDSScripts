BEGIN

select @RasMetricId =RasMetricId,        
	   @MaterialRiskId =MaterialRiskId,
	   @RiskThemeId =RiskThemeId,
	   @MetricId =MetricId,
	   @ZoneContextId=ZoneContextId,
