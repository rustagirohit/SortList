      ,@InputPerformanceValue
      ,@NoOfRecords
      ,@RegionWiseKRIPerformance
      ,@RagFlag
      ,@KRIRemark
      ,@PerformanceInfo
      ,@WeekKey
      ,@CreatedBy
      ,GETUTCDATE();

		  END
		  ELSE
		  BEGIN

